;\r\n`;\r\n\r\nconst LogoText = styled.h1`\r\n    margin: 0;\r\n    padding: 0;\r\n    display: inline;\r\n`;\r\n\r\nconst UserState = styled.div`\r\n    margin-left: auto;\r\n`;\r\n\r\nconst Header = () => {\r\n    let navigate = useNavigate();\r\n    const { data, client } = useQuery(IS_LOGGED_IN);\r\n    return (\r\n        <HeaderBar>\r\n            <img src={logo} alt=\"Notedly Logo\" height=\"40\" />\r\n            <LogoText>Notedly</LogoText>\r\n            {/* If logged in display a logout link, else display sign-in options */}\r\n            <UserState>\r\n                {data.isLoggedIn ? (\r\n                    <ButtonAsLink\r\n                        onClick={() => {\r\n                            // remove the token\r\n                            localStorage.removeItem('token');\r\n                            // clear the application's cache\r\n                            client.resetStore();\r\n                            // update local state\r\n                            isLoggedInVar(false);\r\n                            // redirect the user to the home page\r\n                            navigate(\"/\");\r\n                        }}>Logout</ButtonAsLink>\r\n                ) : (\r\n                    <p>\r\n                        <Link to={'/signin'}>Sign In</Link> or{' '}\r\n                        <Link to={'/signup'}>Sign Up</Link>\r\n                    </p>\r\n                )}\r\n            </UserState>\r\n        </HeaderBar>\r\n    );\r\n};\r\n\r\n// wrap our component in the withRouter higher-order component\r\n// when we want to include routing in a component that is not\r\n// itself directly routable, we need to use withRouter\r\nexport default Header;"},"lineCount":null}},"error":null,"hash":"2c885a1fea6e628e8c06d332b2395f43","cacheData":{"env":{}}}