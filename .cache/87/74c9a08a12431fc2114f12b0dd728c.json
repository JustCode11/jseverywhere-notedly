{\r\n                        <PrivateRoute>\r\n                            <MyNotes />\r\n                        </PrivateRoute>\r\n                    } />\r\n                    <Route path=\"/favorites\" element={\r\n                        <PrivateRoute>\r\n                            <Favorites />\r\n                        </PrivateRoute>\r\n                    } />\r\n                    <Route path=\"/note/:id\" element={<NotePage />} />\r\n                    <Route path=\"/signup\" element={<SignUp />} />\r\n                    <Route path=\"/signin\" element={<SignIn />} />\r\n                    <Route path=\"/new\" element={\r\n                        <PrivateRoute>\r\n                            <NewNote />\r\n                        </PrivateRoute>\r\n                    } />\r\n                    <Route path=\"/edit/:id\" element={\r\n                        <PrivateRoute>\r\n                            <EditNote />\r\n                        </PrivateRoute>\r\n                    } />\r\n                </Routes>\r\n            </Layout>\r\n        </Router>\r\n    );\r\n};\r\n\r\nconst PrivateRoute = ({ children }) => {\r\n    const { loading, error, data } = useQuery(IS_LOGGED_IN);\r\n\r\n    if (loading) return <p>Loading...</p>;\r\n    if (error) return <p>Error!</p>;\r\n    return (\r\n        <React.Fragment>\r\n            {data.isLoggedIn === true ? children : <Navigate to=\"/signin\" />}\r\n        </React.Fragment>\r\n    )\r\n}\r\n\r\n/*const PrivateRoute = ({ component: Component, ...rest }) => {\r\n    const { loading, error, data } = useQuery(IS_LOGGED_IN);\r\n\r\n    if (loading) return <p>Loading...</p>;\r\n    if (error) return <p>Error!</p>;\r\n    // if the user is logged in, route them to the requested component\r\n    // else redirect them to the sign-in page\r\n    return (\r\n        <Route\r\n            {...rest}\r\n            render={props =>\r\n                data.isLoggedIn === true ? (\r\n                    <Component {...props} />\r\n                ) : (\r\n                    <Redirect to={{\r\n                        pathname: '/signin',\r\n                        state: { from: props.location }\r\n                    }}\r\n                    />\r\n                )\r\n            }\r\n        />\r\n    );\r\n};*/\r\n\r\nexport default Pages;"},"lineCount":null}},"error":null,"hash":"66db0a6d010cb33703e4ef521b4980d1","cacheData":{"env":{}}}